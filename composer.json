{
    "name": "bad_cluster/runway",
    "type": "library",
    "description": "A Symfony-like platform for building websites and web apps.",
    "keywords": ["runway"],
    "license": "BSD-3-Clause",
    "authors": [
        {"name": "BaD ClusteR", "email": "admin@badcluster.ru"}
    ],
    "require": {
        "php": ">=8.4",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-yaml": "*",
        "ext-zip": "*",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {"Runway\\" : "src/"},
        "files": ["bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {"Runway\\Tests\\": "tests/"}
    },
    "config": {
        "sort-packages": true
    }
}
